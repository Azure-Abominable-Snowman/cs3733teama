<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="loginPlane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="boundaries.StaffLoginController">
   <children>
      <JFXTextField fx:id="IDfield" layoutX="188.0" layoutY="120.0" prefHeight="27.0" prefWidth="211.0" promptText="Username" />
      <JFXButton fx:id="login" layoutX="135.0" layoutY="295.0" onAction="#onLoginClick" prefHeight="57.0" prefWidth="98.0" ripplerFill="BLACK" style="-fx-background-color: #ff0000;" text="Log In" />
      <JFXButton fx:id="back" layoutX="245.0" layoutY="295.0" onAction="#onBackClick" prefHeight="57.0" prefWidth="98.0" ripplerFill="BLACK" style="-fx-background-color: #add8e6;" text="Back" />
      <JFXPasswordField fx:id="passwordField" layoutX="185.0" layoutY="200.0" onKeyPressed="#onEnterPressed" prefHeight="27.0" prefWidth="229.0" promptText="Password" />
      <Label fx:id="header" contentDisplay="CENTER" layoutX="278.0" layoutY="44.0" prefHeight="45.0" prefWidth="42.0" text="Log In" textAlignment="RIGHT" />
      <JFXButton fx:id="reset" layoutX="350.0" layoutY="295.0" onAction="#onResetClick" prefHeight="57.0" prefWidth="98.0" ripplerFill="BLACK" style="-fx-background-color: #FFD700;" text="Reset" />
      <Label fx:id="loginMessage" layoutX="179.0" layoutY="255.0" prefHeight="17.0" prefWidth="229.0" />
   </children>
</AnchorPane>
